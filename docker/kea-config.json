{
    "Dhcp4": {
        "valid-lifetime": 4000,
        "renew-timer": 1000,
        "rebind-timer": 2000,
        "interfaces-config": {
            "interfaces": [
                "enp4s0f0"
            ]
        },
        "subnet4": [
            {
                "subnet": "192.168.0.0/24",
                "pools": [
                    {
                        "pool": "192.168.0.10 - 192.168.0.200"
                    }
                ],
                "option-data": [
                    {
                        "name": "domain-name-servers",
                        "data": "8.8.8.8"
                    },
                    {
                        "name": "routers",
                        "data": "192.168.0.254"
                    }
                ]
            }
        ],
        "next-server": "192.168.0.254",
        "boot-file-name": "pxelinux.0"
    },
    "Logging": {
        "loggers": [
            {
                "name": "kea-dhcp4",
                "output_options": [
                    {
                        "output": "stdout"
                    }
                ],
                "severity": "DEBUG"
            }
        ]
    }
}
